<div class="container">
  <h1>User List</h1>
  <div class="row"> 
    <% @users.each do |user| %>
      <div class="col-xs-12">
        <h3 class="col-xs-8"> <%= user.email %>  </h3>
        <h3 class="col-xs-4"><%= link_to 'Edit', edit_user_path(user) %></h3>
      </div>
    <% end %>
    <div class="col-xs-10">
      <h3> <%= link_to 'New User', new_user_path %></p>
    </div>
  </div>
</div>